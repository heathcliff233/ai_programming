    "x = int(input())\n",
    "y=x\n",
    "print(y,'= ',end='')\n",
    "while(x != 1):\n",
    "    t = x\n",
    "    i = 2\n",
    "    while(bool(t == x)):\n",
    "        if(x%i==0):\n",
    "            x /= i\n",
    "            if(x==1):\n",
    "                print(i)\n",
    "            else:\n",
    "                print(\"%d*\"%i,end='')\n",
    "        else:\n",
    "            i += 1"
