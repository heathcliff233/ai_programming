    "def f(x,i):\n",
    "    y = x\n",
    "    for j in range (2*i-2):\n",
    "        x *= y\n",
    "    x *= -pow(-1,i)\n",
    "    for k in range (1,2*i):\n",
    "        x/=k\n",
    "    return x"
   ]
